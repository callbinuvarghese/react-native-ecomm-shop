{
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production ts-node src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node seed-data.js",
    "db:setup": "node apply-schema.js && node seed-data.js",
    "images:download": "node download-images.js",
    "images:update-seed": "node update-seed-data.js",
    "images:setup": "node update-seed-data.js && node download-images.js"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@types/ws": "^8.18.1",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.29.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
